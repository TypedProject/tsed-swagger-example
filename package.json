{
  "name": "tsed-swagger-example",
  "version": "1.0.0",
  "description": "An application example with Ts.ED and @tsed/swagger",
  "scripts": {
    "build": "yarn tsc",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "yarn test:lint && yarn test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn test:unit",
    "test:lint": "eslint src --ext .ts",
    "test:lint:fix": "eslint src --ext .ts --fix",
    "prettier": "prettier '{src,test}/**/*.ts' --write"
  },
  "dependencies": {
    "@tsed/ajv": "6.2.2",
    "@tsed/common": "6.2.2",
    "@tsed/core": "6.2.2",
    "@tsed/di": "6.2.2",
    "@tsed/exceptions": "6.2.2",
    "@tsed/schema": "6.2.2",
    "@tsed/json-mapper": "6.2.2",
    "@tsed/platform-express": "6.2.2",
    "@tsed/swagger": "6.2.2",
    "ajv": "6.12.5",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "cross-env": "7.0.2",
    "express": "4.17.1",
    "method-override": "3.0.0"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "2.0.0",
    "@tsed/cli-plugin-jest": "2.0.0",
    "@types/compression": "1.7.0",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.8",
    "@types/express": "4.17.8",
    "@types/jest": "26.0.14",
    "@types/method-override": "0.0.31",
    "@types/node": "14.11.8",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.4.0",
    "@typescript-eslint/parser": "4.4.0",
    "concurrently": "5.3.0",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "jest": "26.5.2",
    "lint-staged": "10.4.0",
    "nodemon": "2.0.4",
    "prettier": "2.1.2",
    "supertest": "5.0.0",
    "ts-jest": "26.4.1",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "keywords": []
}
